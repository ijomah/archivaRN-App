{
  "expo": {
    "name": "pixcapt",
    "slug": "pixcapt",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/archiva-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/archiva-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/arch-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jacobianijo.pixcapt"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "react-native-document-scanner-plugin"
    ],
    "extra": {
      "eas": {
        "projectId": "458f5141-20f7-49c0-aeb5-d46cc8ee9667"
      }
    }
  }
}
